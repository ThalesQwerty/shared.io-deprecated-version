{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "es5",
            "es6",
            "dom"
        ],
        "declaration": true,
        "sourceMap": true,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}